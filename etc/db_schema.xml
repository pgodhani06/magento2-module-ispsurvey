	<table name="sales_order" resource="sales" engine="innodb" comment="Sales Order">
		<column xsi:type="varchar" length="1000" name="is_satisfied" nullable="true" comment="Is Satisfied"/>
		<column xsi:type="varchar" length="50" name="isp" nullable="true" comment="Isp"/>
